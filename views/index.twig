{% extends "base.twig" %}
{% block content %}

    {% for post in posts %}

    <div class="text-center col-xs-12 col-sm-6 col-md-4 col-lg-3">

        <div class="img_box1">
            <p class="img_comment1">{{post.user.fullname}}</p>
            <a target="_blank" href="{{post.post.link}}">
                <img  src="{{post.post.imageUrl}}" width="300px">
            </a>
        </div>
    </div>

    {% endfor %}

{% endblock %}
